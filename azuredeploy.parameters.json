{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"_artifactsLocation": {
			"value": "https://raw.githubusercontent.com/mglantz/ocp36-azure-simple/master/"
		},
		"masterVmSize": {
			"value": "Standard_DS4_v2"
		},
		"nodeVmSize": {
			"value": "Standard_DS3_v2"
		},
		"openshiftClusterPrefix": {
			"value": "ocp"
		},
		"openshiftMasterPublicIpDnsLabel": {
			"value": "master"
		},
		"infraLbPublicIpDnsLabel": {
			"value": "apps"
		},
		"masterInstanceCount": {
			"value": 1
		},
		"nodeInstanceCount": {
			"value": 1
		},
		"dataDiskSize": {
			"value": 128
		},
		"adminUsername": {
			"value": "admin-user"
		},
		"openshiftPassword": {
			"value": "PICK_AN_ADMIN_PASSWORD"
		},
		"cloudAccessUsername": {
			"value": "YOUR_REDHAT_ACCESS_EMAIL"
		},
		"cloudAccessPassword": {
			"value": "YOUR_REDHAT_ACCESS_PASSWORD"
		},
		"cloudAccessPoolId": {
			"value": "YOUR_REDHAT_OPENSHIFT_SUBSCRIPTION_POOL_ID"
		},
		"sshPublicKey": {
			"value": "GENERATE_A_PUBLIC_SSH_KEY"
		},
		"keyVaultResourceGroup": {
			"value": "ocp"
		},
		"keyVaultName": {
			"value": "ocpKeyVaultName"
		},
		"keyVaultSecret": {
			"value": "ocpSecretName"
		},
		"defaultSubDomainType": {
			"value": "custom"
		},
		"defaultSubDomain": {
			"value": "YOUR_PUBLIC_DOMAIN_NAME"
		}
	}
}
